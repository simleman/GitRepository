<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="">
  <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
  <property name="hibernate.connection.password">111111</property>
  <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/test</property>
  <property name="hibernate.connection.username">root</property>
  <property name="hibernate.dialect">org.hibernate.dialect.MySQLInnoDBDialect</property>
  <property name="hibernate.show_sql">true</property>
  <property name="hibernate.format_sql">true</property>
  <mapping class="demo.entity.Mydemo" resource="demo/entity/Mydemo.hbm.xml"/>
  <mapping class="demo.entity.Paper" resource="demo/entity/Paper.hbm.xml"/>
  <mapping class="demo.entity.Questiontype" resource="demo/entity/Questiontype.hbm.xml"/>
  <mapping class="demo.entity.Station" resource="demo/entity/Station.hbm.xml"/>
  <mapping class="demo.entity.Testquestions" resource="demo/entity/Testquestions.hbm.xml"/>
  <mapping class="demo.entity.Userclass" resource="demo/entity/Userclass.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
